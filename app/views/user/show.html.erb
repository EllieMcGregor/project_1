<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Role</th>
      <th>DOB</th>
      <th>Image </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <td><%= @user.first_name %></td>
      <td><%= @user.last_name %></td>
      <td><%= @user.email %></td>
      <td><%= @user.role %></td>
      <td><%= @user.dob %></td>
      <td><%= image_tag @user.user_image %></td>
      <td><%= link_to 'Edit', edit_user_registration_path(@user) %></td>
    </tr>
  </tbody>
</table>
