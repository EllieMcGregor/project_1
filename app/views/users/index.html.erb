<%= search_form_for @q do |f| %>
  <p>
    <%= f.label :first_name_cont %>
    <%= f.text_field :first_name_cont %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>


<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Role</th>
      <th>Image </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>

    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.email %></td>
      <td><%= user.role %></td>
      <td><%= image_tag user.user_image %></td> 
      <td><%= link_to 'Edit', edit_user_registration_path(@user) %></td>
      <tr>
        <% end %>

      </tbody>
    </table>
